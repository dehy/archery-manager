{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4.0",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-gettext": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "api-platform/core": "^3.0",
        "async-aws/simple-s3": "^2.0",
        "cocur/slugify": "^4.2",
        "damienharper/auditor-bundle": "^6.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^3.0",
        "dragonmantank/cron-expression": "^3.3",
        "easycorp/easyadmin-bundle": "^4.1",
        "fresh/doctrine-enum-bundle": "^11.0",
        "gedmo/doctrine-extensions": "^3.9",
        "geocoder-php/mapbox-provider": "^1.4",
        "knplabs/knp-time-bundle": "^2.1",
        "knpuniversity/oauth2-client-bundle": "^2.10",
        "league/commonmark": "^2.8",
        "league/flysystem-async-aws-s3": "^3.0",
        "league/flysystem-bundle": "^3.0",
        "league/flysystem-memory": "^3.29",
        "moneyphp/money": "^4.0",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "ralouphie/mimey": "^1.0",
        "sentry/sentry-symfony": "^5.8",
        "smalot/pdfparser": "^2.2",
        "specshaper/encrypt-bundle": "^5.0",
        "symfony/asset": "*",
        "symfony/browser-kit": "*",
        "symfony/console": "*",
        "symfony/doctrine-messenger": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^2",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "*",
        "symfony/intl": "*",
        "symfony/mailer": "*",
        "symfony/mailgun-mailer": "*",
        "symfony/mime": "*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "*",
        "symfony/process": "*",
        "symfony/property-access": "*",
        "symfony/property-info": "*",
        "symfony/proxy-manager-bridge": "*",
        "symfony/runtime": "*",
        "symfony/scaleway-mailer": "*",
        "symfony/scheduler": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer": "*",
        "symfony/stimulus-bundle": "^2.11",
        "symfony/string": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/ux-chartjs": "^2.6",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfony/yaml": "*",
        "symfonycasts/reset-password-bundle": "^1.13",
        "symfonycasts/verify-email-bundle": "^1.10",
        "team-reflex/discord-php": "^10.0",
        "twig/cssinliner-extra": "^3.4",
        "twig/extra-bundle": "^3.5",
        "twig/inky-extra": "^3.4",
        "twig/intl-extra": "^3.3",
        "twig/string-extra": "^3.5",
        "twig/twig": "^2.12|^3.0",
        "vich/uploader-bundle": "^2.0",
        "willdurand/geocoder-bundle": "^5.0",
        "wohali/oauth2-discord-new": "^1.2"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.4.*",
            "docker": true
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.2",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.14",
        "hautelook/alice-bundle": "^2.11",
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-phpunit": "^1.4",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^1.2",
        "symfony/css-selector": "*",
        "symfony/debug-bundle": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*"
    }
}
